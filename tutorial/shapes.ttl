hpfs:air-bidirectional-connected-toAnnotationRule a sh:SPARQLRule ;
    sh:construct """
                CONSTRUCT {
                    ?name <urn:hpflex/shapes#air-bidirectional-connected-to> ?target .
                }
                WHERE {
	?target <http://data.ashrae.org/standard223#connectedTo> ?name . 
	?name <http://data.ashrae.org/standard223#connectedTo> ?target . 
}""" .

hpfs:air-bidirectional-cpAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:air-bidirectional-cp ;
    sh:object hpfs:air-bidirectional-cp ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:air-bidirectional-cp_Medium-Air a sh:PropertyShape ;
    rdfs:label "air-bidirectional-cp_Medium-Air" ;
    sh:path s223:hasMedium ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:air-bidirectional-cp_Medium-Air-1 .

hpfs:air-bidirectional-cp_Medium-Air-1 a sh:NodeShape ;
    rdfs:label "air-bidirectional-cp_Medium-Air-1" ;
    sh:hasValue s223:Medium-Air .

hpfs:air-bidirectional-cp_hasMedium rdfs:label "air-bidirectional-cp_hasMedium" ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path s223:hasMedium .

hpfs:air-bidirectional-cp_noAspects rdfs:label "air-bidirectional-cp_noAspects" ;
    sh:maxCount 0 ;
    sh:minCount 0 ;
    sh:path s223:hasAspect .

