@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hpfs:air-bidirectional-cp a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    sh:class s223:BidirectionalConnectionPoint ;
    sh:property hpfs:air-bidirectional-cp_Medium-Air,
        hpfs:air-bidirectional-cp_hasMedium,
        hpfs:air-bidirectional-cp_noAspects .

